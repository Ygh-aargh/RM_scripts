#!/usr/bin/gnuplot -persist
f(x) = a + b * sqrt(x)
fit [1:] f(x) "kill_merit" u ($1):($2) via a, b
se ke le to
se xla "M kills"
se yla "merit"
p [][:] "kill_merit" u ($1):($2) t "data", "" u ($1):(($2-f($1))*100) t "residual * 100", -200+700*sqrt(x)
exit

