#!/usr/bin/gnuplot -persist
se xla "level"
se yla "amount of 50k (green) books required"
se log y
se grid
se te png
se ou "gen_level.png"
se ke le Le rev
p [38:145][1:50000] "< awk '{n+=$2; if (NF>=2 && $1<=141) print $1+1, $2, n, (n/50000)}' gen_level | grep -v awk" \
u ($1):($4) t "from level 0", "" u ($1):($2/50000) t "from previous level"
se ou
exit

