#!/usr/bin/gnuplot -persist
se xla "L1 gems to build"
se xtics (0, "" 64 1, "" 128 1, "" 256 1, "" 512 1, "" 1024 1, \
        "" 2048 1, "" 4096, "8k" 8192, "  16k" 16384 )
se mxtics 4
se x2tics ("" 16884 1, "" 17484 1, "" 18284 1, "19284      " 19284, \
        "" 20484 1, "" 21984 1, "" 23784 1, "" 25984 1, "     28584" 28584, \
        "" 31584 1, "" 34684 1, "" 37924 1, "" 41424 1, "      45124" 45124, \
        "" 48904 1, "" 52994 1, "" 57394 1, "" 62594 1, "      68694" 68694, \
        "" 75894 1, "" 80314 1, "" 85094 1, "" 90264 1, "      95854" 95854, \
	"" 101894 1, "" 108424 1, "" 115484 1, "" 123114 1, 131434, \
	"" 140344 1, "" 149974 1, "" 160384 1, "" 171634 1, 183784, \
	"" 196914 1, "" 211104 1, "" 226434 1, "" 242994 1, 260884 )
se yla "equivalent treasure levels"
se ytics (0, "" 6 1, "" 8 1, "" 11 1, "" 15 1, "" 21 1, \
        30, 45, 60, 80, 100, 120, \
        "" 125 1, "" 130 1, "" 135 1, 140, \
        "" 145 1, "" 150 1, "" 155 1, "" 160 1, 165, \
        "" 170 1, "" 175 1, "" 180 1, "" 185 1, 200, \
        "" 205 1, "" 210 1, "" 215 1, "" 220 1, 230, \
        "" 235 1, "" 240 1, "" 245 1, "" 250 1, 255, \
	"" 260 1, "" 265 1, "" 270 1, "" 275 1, 290, \
	"" 295 1, "" 300 1, "" 305 1, "" 310 1, 320, \
	"" 325 1, "" 330 1, "" 335 1, "" 340 1, 350 )
se pointsize 1.7
se ke bo
se label 1  " L10"       at 512,     26 left
se label 2  " L11"       at 1024,    41 left
se label 3  " L12"       at 2048,    56 left
se label 4  " L13"       at 4096,    76 left
se label 5  " L14"       at 8192,    96 left
se label 6  " L15 / L1*" at 15384,  110 left
se label 7  " L5*"       at 18284,  130 left
se label 8  " L10*"      at 27584,  155 left
se label 9  " L15*"      at 44124,  190 left
se label 10 " L20*"      at 67694,  220 left
se label 11 " L25*"      at 94854,  245 left
se label 12 " L30*"      at 130434, 280 left
se label 13 " L35*"      at 182784, 310 left
se label 14 " L40*"      at 259884, 340 left
se label 21 "*5  for  attack"  at 4096, 340 left
se label 22 "*3  for  defense" at 4096, 325 left
se label 23 "*8  for  troops"  at 4096, 310 left
se te png size 1000,600
se ou "gem_build.png"
p [:275000][:360] "gem_build" u ($2):($1) eve :::1::1 t "classic gems" w lp pt 4, \
  "" u 2:1 eve :::2::2 t "star gems" w lp pt 3, \
  "" u 2:1 eve :::3::3 t "pre-L140 star gems" w lp pt 1
se ou
exit

